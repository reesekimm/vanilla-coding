<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <title><%= htmlWebpackPlugin.options.title %></title>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css"
  integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/css?family=Noto+Sans+KR:300|Roboto:100,100i,400&display=swap&subset=korean"
  rel="stylesheet">
</head>
<body>
  <h1 class="title">Sorting Algorithms</h1>
  <div class="content">
    <section class="menu">
      <div class="menu_selection_form">
        <select class="menu_selection">
            <option value="bubble">Bubble Sort</option>
            <option value="quick">Quick Sort</option>
        </select>
      </div>
      <form class="menu_input">
          <input type="text" id="myNums" placeholder="Enter numbers hear     e.g) 3, 7, 11, 2, 6">
      </form>
      <button class="menu_start-btn">Go</button>
    </section>
    <div class="canvas">
    </div>
  </div>
</body>
</html>
